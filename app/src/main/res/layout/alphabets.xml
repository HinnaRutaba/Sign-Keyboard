<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary">


    <ImageButton
        android:id="@+id/btn_a"
        android:src="@drawable/s_a"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="2dp"
        />

    <ImageButton
        android:id="@+id/btn_b"
        android:src="@drawable/s_b"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_toRightOf="@+id/btn_a"
        />

    <ImageButton
        android:id="@+id/btn_c"
        android:src="@drawable/s_c"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_toRightOf="@+id/btn_b"
        />

    <ImageButton
        android:id="@+id/btn_d"
        android:src="@drawable/s_d"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_toRightOf="@+id/btn_c"
        />

    <ImageButton
        android:id="@+id/btn_e"
        android:src="@drawable/s_e"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_toRightOf="@+id/btn_d"
        />

    <ImageButton
        android:id="@+id/btn_f"
        android:src="@drawable/s_f"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_toRightOf="@+id/btn_e"
        />

    <ImageButton
        android:id="@+id/btn_g"
        android:src="@drawable/s_g"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@+id/btn_a"
        android:layout_marginLeft="2dp"
        />

    <ImageButton
        android:id="@+id/btn_h"
        android:src="@drawable/s_h"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_b"
        android:layout_toRightOf="@id/btn_g"
        />

    <ImageButton
        android:id="@+id/btn_i"
        android:src="@drawable/s_i"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_c"
        android:layout_toRightOf="@id/btn_h"/>

    <ImageButton
        android:id="@+id/btn_j"
        android:src="@drawable/s_j"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_d"
        android:layout_toRightOf="@id/btn_i"
        />

    <ImageButton
        android:id="@+id/btn_k"
        android:src="@drawable/s_k"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_e"
        android:layout_toRightOf="@id/btn_j"
        />

    <ImageButton
        android:id="@+id/btn_l"
        android:src="@drawable/s_l"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_f"
        android:layout_toRightOf="@id/btn_k"
        />

    <ImageButton
        android:id="@+id/btn_m"
        android:src="@drawable/s_m"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_g"
        android:layout_marginLeft="2dp"
        />

    <ImageButton
        android:id="@+id/btn_n"
        android:src="@drawable/s_n"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_h"
        android:layout_toRightOf="@id/btn_m"
        />

    <ImageButton
        android:id="@+id/btn_o"
        android:src="@drawable/s_o"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_i"
        android:layout_toRightOf="@id/btn_n"
        />

    <ImageButton
        android:id="@+id/btn_p"
        android:src="@drawable/s_p"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_j"
        android:layout_toRightOf="@id/btn_o"
        />

    <ImageButton
        android:id="@+id/btn_q"
        android:src="@drawable/s_q"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_i"
        android:layout_toRightOf="@id/btn_p"
        />

    <ImageButton
        android:id="@+id/btn_r"
        android:src="@drawable/s_r"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_j"
        android:layout_toRightOf="@id/btn_q"
        />

    <ImageButton
        android:id="@+id/btn_s"
        android:src="@drawable/s_s"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_m"
        android:layout_marginLeft="2dp"
        />

    <ImageButton
        android:id="@+id/btn_t"
        android:src="@drawable/s_t"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_n"
        android:layout_toRightOf="@id/btn_s"
        />

    <ImageButton
        android:id="@+id/btn_u"
        android:src="@drawable/s_u"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_o"
        android:layout_toRightOf="@id/btn_t"
        />

    <ImageButton
        android:id="@+id/btn_v"
        android:src="@drawable/s_v"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_p"
        android:layout_toRightOf="@id/btn_u"
        />

    <ImageButton
        android:id="@+id/btn_w"
        android:src="@drawable/s_w"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_q"
        android:layout_toRightOf="@id/btn_v"
        />

    <ImageButton
        android:id="@+id/btn_x"
        android:src="@drawable/s_x"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_r"
        android:layout_toRightOf="@id/btn_w"
        />

    <ImageButton
        android:id="@+id/btn_y"
        android:src="@drawable/s_y"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_s"
        android:layout_marginLeft="2dp"
        />

    <ImageButton
        android:id="@+id/btn_z"
        android:src="@drawable/s_z"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_t"
        android:layout_toRightOf="@id/btn_y"
        />

    <ImageButton
        android:id="@+id/btn_spaceA"
        android:src="@drawable/space"
        android:layout_width="182dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_u"
        android:layout_toRightOf="@id/btn_z"
        />

    <ImageButton
        android:id="@+id/btn_removeA"
        android:src="@drawable/cancel"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_below="@id/btn_x"
        android:layout_toRightOf="@id/btn_space"
        />



</RelativeLayout>